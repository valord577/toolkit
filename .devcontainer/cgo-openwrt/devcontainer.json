{
  "name": "cgo openwrt",
  "image": "valord577/cgo-openwrt-mt7622:ef93d18ca03a1aea7c9d009716268df51ac22de3",
  "runArgs": [
    "--name=cgo-openwrt-${devcontainerId}"
  ],
  "mounts": [
    "source=${localEnv:HOME}/go,target=/root/go,type=bind"
  ],
  "postAttachCommand": {
    "0": "go env -w GOPROXY=https://goproxy.cn,direct",
    "1": "go env -w GOSUMDB=sum.golang.google.cn",
  },
  "customizations": {
    "vscode": {
      "settings": {
        "git.autorefresh": true
      },
      "extensions": [
        "golang.go-nightly"
      ]
    }
  }
}
